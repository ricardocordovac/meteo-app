{
  "version": 2,
  "framework": "angular",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/ng"
    },
    {
      "src": "api/MeteoApi/*.csproj",
      "use": "@vercel/dotnet"
    }
