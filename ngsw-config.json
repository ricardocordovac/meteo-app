
{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(jpg|png|svg)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "supabase-api",
      "urls": ["https://mdsirweaayrcqhyuxoxa.supabase.co/rest/v1/current_data*"],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "1h",
        "strategy": "freshness"
      }
    }
  ]
}
